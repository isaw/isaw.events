          </tr>
        </tal:groups>
        <!--> </-->
        <tr>
          <td>
            <h4>
              <a href="setProdMode.html">Production mode</a>
            </h4>
          </td>
        </tr>
        <tal:groups repeat="selenium_test selenium_tests">
          <tr>
            <td><a href="test.html"
                   tal:content="selenium_test/title"
                   tal:attributes="href selenium_test/href"
                   >Title</a></td>
          </tr>
        </tal:groups>
      </tbody>
    </table>
